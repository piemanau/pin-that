{
  "schemaVersion": 1,
  "id": "pinthat",
  "version": "${version}",
  "name": "Pin That!",
  "description": "This mod lets you pin text on any corner of the HUD.",
  "authors": [
    "LimeShulkerBox"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/pin-that",
    "sources": "https://github.com/LimeShulkerBox/pin-that",
    "issues": "https://github.com/LimeShulkerBox/pin-that/issues"
  },
  "license": "MIT",
  "icon": "assets/pinthat/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.limeshulkerbox.pinthat.ModInitializer"
    ],
    "modmenu": [
      "com.limeshulkerbox.pinthat.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": "*",
    "minecraft": "1.19.x"
  }
}
